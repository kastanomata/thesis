      </table:table-cell>
      <table:table-cell table:style-name="Table5.A1" office:value-type="string">
       <text:p text:style-name="Table_20_Heading"><text:span text:style-name="Strong_20_Emphasis">Descrizione</text:span></text:p>
      </table:table-cell>
     </table:table-row>
